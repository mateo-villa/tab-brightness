{
    "manifest_version": 3,
    "name": "Simple Extension",
    "description": "This is a simple extension",
    "version": "0.0.1",
    "action": {
      "default_popup": "popup/popup.html"
    },
    "permissions": ["activeTab", "scripting"],
    "commands": {
      "_execute_action":{
      "suggested_key": {
        "default": "Ctrl+Q",
        "description": "Keyboard shortcut for slider"
        }
      }
    }
  }
